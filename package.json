{
  "name": "ngx-daterangepicker-material",
  "version": "6.0.4",
  "description": "Angular 9 and 10 date range picker (with material design theme)",
  "keywords": [
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "angular8",
    "angular9",
    "angular10",
    "angular11",
    "angular12",
    "angular13",
    "ng2",
    "daterangepicker",
    "material",
    "component",
    "directive",
    "daterange",
    "date",
    "dayjs",
    "picker"
  ],
  "homepage": "https://github.com/fetrarij/ngx-daterangepicker-material",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "demo:build": "ng build --configuration production",
    "test": "./node_modules/.bin/ng test --watch=false",
    "lint": "ng lint",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.json",
    "gh-pages": "ng build --configuration production --base-href=\"/ngx-daterangepicker-material/\" && ngh --dir demo/dist",
    "build": "ng-packagr -p ng-package.json -c tsconfig.json",
    "build:schematics": "tsc -p ./tsconfig.schematics.json",
    "copy:schematics": "cpr schematics/dist dist/schematics && cp ./schematics/collection.json ./dist/schematics && cp ./schematics/ng-add/schema.json ./dist/schematics/ng-add",
    "postbuild": "npm run build:schematics && npm run copy:schematics"
  },
  "author": "Fetrarijaona R.",
  "repository": {
    "type": "git",
    "url": "https://github.com/fetrarij/ngx-daterangepicker-material.git"
  },
  "peerDependencies": {
    "dayjs": "^1.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.15",
    "@angular-eslint/builder": "19.3.0",
    "@angular-eslint/eslint-plugin": "19.3.0",
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-eslint/schematics": "19.3.0",
    "@angular-eslint/template-parser": "19.3.0",
    "@angular/animations": "^19.2.3",
    "@angular/cli": "^18.2.15",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^19.2.3",
    "@angular/compiler": "^19.2.3",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^19.2.3",
    "@angular/compiler-cli": "^19.2.3",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^19.2.3",
    "@angular/platform-browser-dynamic": "^19.2.3",
    "@angular/router": "^19.2.3",
    "@schematics/angular": "^11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.18.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "angular-cli-ghpages": "^1.0.3",
    "core-js": "^2.5.4",
    "cpr": "^3.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "materialize-css": "^1.0.0",
    "ng-packagr": "^18.2.1",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "rimraf": "^2.6.1",
    "rxjs": "^6.3.3",
    "ts-node": "~8.3.0",
    "typescript": "^5.8.2",
    "zone.js": "~0.14.10"
  },
  "dependencies": {
    "dayjs": "^1.11.5",
    "tslib": "^2.0.0"
  },
  "schematics": "./schematics/collection.json"
}